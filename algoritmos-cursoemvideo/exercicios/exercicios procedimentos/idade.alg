algoritmo "semnome"
var
C,a, n,idade : Inteiro
procedimento subtracao(A, B:INTEIRO)
inicio
C <- a - b
A := B
B := C
Escreva ("Em ", a, " voce tera ", c , " anos")
Se (idade >= 21) entao
   EscrevaL (" e ja tera atingido a maioridade. ")
FimSe
fimprocedimento
inicio
Escreva("Em que ano estamos? ")
Leia(a)
Escreva("Em que ano voce nasceu? ")
Leia(n)
subtracao(a, n)

fimalgoritmo
